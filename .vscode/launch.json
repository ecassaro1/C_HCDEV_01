{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run cap1-1",
      "request": "launch",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}/cap1",
      "runtimeExecutable": "npx",
      "args": [
        "cds",
        "run",
        "--in-memory?"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/cap1/.env1",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/cap1\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    }  
  ]
}